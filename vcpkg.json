{
  "$schema": "https://raw.githubusercontent.com/microsoft/vcpkg-tool/main/docs/vcpkg.schema.json",
  "name": "arcade-learning-environment",
  "version": "0.10.1",
  "builtin-baseline": "0ca64b4e1c70fa6d9f53b369b8f3f0843797c20c",
  "dependencies": [
    "zlib"
  ],
  "features": {
    "sdl": {
      "description": "Enable SDL, this enables display and audio support.",
      "dependencies": [
        "sdl2"
      ]
    },
    "vector": {
      "description": "Enable vector environment",
      "dependencies": [
        "gflags",
        "glog",
        "opencv2"
      ]
    },
    "cuda": {
      "description": "Enable CUDA, this enable XLA support.",
      "supports": "linux & windows",
      "dependencies": [
        "cuda"
      ]
    }
  },
  "overrides": [
    {
      "name": "sdl2",
      "version": "2.24.2"
    }
  ]
}

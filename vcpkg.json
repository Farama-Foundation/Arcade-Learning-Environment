{
  "$schema": "https://raw.githubusercontent.com/microsoft/vcpkg-tool/main/docs/vcpkg.schema.json",
  "name": "arcade-learning-environment",
  "version": "0.11.0",
  "dependencies": [
    "zlib"
  ],
  "features": {
    "sdl": {
      "description": "Enable SDL, this enables display and audio support.",
      "dependencies": [
        {
          "name": "sdl2",
          "default-features": false,
          "features": ["wayland", "x11"]
        }
      ]
    },
    "vector": {
      "description": "Enable Vector, this enables vectorisation of atari environments",
      "dependencies": [
        "opencv"
      ]
    }
  },
  "builtin-baseline": "0ca64b4e1c70fa6d9f53b369b8f3f0843797c20c",
  "overrides": [
    {
      "name": "sdl2",
      "version": "2.32.4"
    }
  ]
}
